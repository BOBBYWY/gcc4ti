#! /bin/sh
#
# Compile and install the GCC4TI environment registrator
#

isbsd=`uname | grep "BSD"`
if [ "$isbsd" = "" ]; then
	#Assume GNU make
        make=make
else
        make=gmake
fi

echo Installing GCC4TI environment registrator...
cd ../sources/envreg/src/
${make} -e
cp envreg $TIGCC/bin
${make} clean
echo Done.
